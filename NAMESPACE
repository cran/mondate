importFrom(utils, head, head.matrix, tail, tail.matrix)
exportPattern("^[[:alpha:]]+")
exportMethods(
    "[",
    "array",
    "as.character",
    "as.Date",
    "as.numeric",
    "as.POSIXct",
    "as.POSIXlt",
    "c",
	cbind,
    "day",
	mondateDisplayFormat,
	displayFormat,
    "matrix",
    "mean",
    "mondate",
    "month",
    "print",
    "rep",
	rbind,
    "show",
	mondateTimeunits,
	timeunits,
    "unique",
    "year" 
)
exportClasses(
    	mondate

)
S3method(as.data.frame, mondate)
S3method(format, mondate)
S3method(seq, mondate)
S3method(head, mondate)
S3method(tail, mondate)
